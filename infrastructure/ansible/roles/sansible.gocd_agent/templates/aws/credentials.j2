{% for profile in gocd_agent.aws.profiles %}
[{{ profile.name }}]
{% for key, value in profile.config.iteritems() %}
{% if value is mapping %}
{{ key }} =
{% for sub_key, sub_value in value.iteritems() %}
  {{ sub_key }} = {{ sub_value }}
{% endfor %}
{% else%}
{{ key }} = {{ value }}
{% endif %}
{% endfor %}
{% endfor %}
